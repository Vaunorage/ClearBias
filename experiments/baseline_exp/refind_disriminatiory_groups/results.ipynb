   "source": [
    "import pandas as pd\n",
    "import sqlite3\n",
    "from path import HERE\n",
    "conn = sqlite3.connect(HERE.joinpath('experiments/baseline_exp/exp.db'))\n",
    "quer = \"\"\"\n",
    "SELECT\n",
    "    method, AVG(coverage_ratio) avg_coverage_ratio\n",
    "FROM experiment_results\n",
    "GROUP BY method\n",
    "\"\"\"\n",
    "df = pd.read_sql_query(quer, conn)\n",
    "df"
